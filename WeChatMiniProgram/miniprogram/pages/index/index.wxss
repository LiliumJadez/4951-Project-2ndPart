page{
    height: 100%;
  }
   
  .container {
    height: 100%;
    display: flex;
    flex-direction: column;
    padding: 0;
    box-sizing: border-box;
    background-color: #e7e6e6;
  
  } 
   
  /*登录图片*/
  .login-icon{
    flex: none;
  }
  .login-img{
    margin-top: 25px;
    width: 450rpx;
    height: 400rpx;
  }
   
  .header text{
      margin-top: 25px;
      font-size: 25px;
      font-weight:bold;
      color:rgb(0, 0, 0);
      width: 100%;
      height: 170rpx;
      display: flex;
      text-align:center;
  }

  /*按钮*/
  .loginBtnView {
    width: 100%;
    height: auto;
    background-color: #e7e6e6;
    margin-top: 0px;
    margin-bottom: 0px;
    padding-bottom: 0px;
  }
   
  .navigateBtn {
    width: 80%;
    margin-top: 35px;
  }